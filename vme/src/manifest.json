{
	"manifest_version": 2,
	"name": "MWW MP305 VME",
	"version": "305",
	"description": "Oficial Mr. Worldwide Mp305 Volume Manager Extension.",

	"permissions": [
		"activeTab",
		"tabCapture"
	],
	
	"browser_action": {
		"default_popup": "popup.html"
	},

	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	
	"icons": {
		"128": "mww.png"
	}
}